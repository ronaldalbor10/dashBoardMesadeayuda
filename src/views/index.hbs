

                    <!-- Page Heading -->
                    <div class="d-sm-flex align-items-center justify-content-between mb-4">
                        <h1 class="h3 mb-0 text-gray-800">Dashboard</h1>
                        <a href="#" id="filtroBtn" class="d-none d-sm-inline-block btn btn-sm btn-primary shadow-sm" title="Ocultar"><i
                                class="fas fa-filter fa-sm text-white-50"></i> Filtrar</a>
                    </div>
                    <form action="/" method="post">
                        <div class="mb-4" id="filtroDV">
                            <div class="card border-left-secondary shadow h-100 py-2">
                                <div class="card-body">
                                    <div class="row no-gutters align-items-center">
                                        <div class="col-md-6">
                                            <div class="mb-2">
                                                <label for="fecha1" class="form-label text-xs font-weight-bold text-secondary text-uppercase">Fecha inicial</label>
                                                <input type="date" name="fecha1" class="form-control form-control-sm" id="fecha1">
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="mb-2">
                                                <label for="fecha2" class="form-label text-xs font-weight-bold text-secondary text-uppercase">Fecha final</label>
                                                <input type="date" name="fecha2" class="form-control form-control-sm" id="fecha2">
                                            </div>
                                        </div>
                                    </div>
                                    {{#if (ifCond user.isadmin 1 true) }}
                                    <div class="row no-gutters align-items-center">
                                        <div class="col">
                                            <div class="mb-2">
                                                <label for="agente" class="form-label text-xs font-weight-bold text-secondary text-uppercase">Seleccione el agente</label>
                                                
                                                <input class="form-control form-control-sm" list="agentes" name="agente" id="agente" value="{{agente}}">
                                                <datalist id="agentes">
                                                    {{#each agentes}}
                                                    <option value="{{this.staff_id}}">{{capitalWords this.firstname}} {{capitalWords this.lastname}}</option>
                                                    {{/each}}
                                                </datalist>    
                                            </div>
                                        </div>
                                    </div>
                                    {{/if}}
                                    <div class="row no-gutters align-items-center">
                                        <div class="d-grid gap-2">
                                            <button class="btn btn-primary btn-sm" type="submit"><i class="fas fa-search fa-sm text-white-50 me-2"></i>Buscar</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                    

                    
                    {{#if (ifCond user.isadmin 1 true) }}
                        {{>dashboardAdmin}}
                    {{else}}
                        {{>dashboardAgent}}
                    {{/if}}

                    

                    <div class="row">

