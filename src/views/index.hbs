                                            <a class="dropdown-item" href="#myPieChartTxS" onclick="ajaxGetPieChart('myPieChartTxS','ticketxservicio','overdue');">Fuera de tiempo</a>
                                            <a class="dropdown-item" href="#myPieChartTxS" onclick="ajaxGetPieChart('myPieChartTxS','ticketxservicio','closed');">Cerrados</a>
                                            <a class="dropdown-item" href="#myPieChartTxS" onclick="ajaxGetPieChart('myPieChartTxS','ticketxservicio','');">Todos</a>
                                            <!--<a class="dropdown-item" href="#">Another action</a>
                                            <div class="dropdown-divider"></div>
                                            <a class="dropdown-item" href="#">Something else here</a>-->
                                        </div>
                                    </div>
                                </div>
                                <!-- Card Body -->
